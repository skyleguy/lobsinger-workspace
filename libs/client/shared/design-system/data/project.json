{
  "name": "design-system",
  "$schema": "../../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/client/shared/design-system/data/src",
  "projectType": "library",
  "tags": ["scope:client", "type:data"],
  "targets": {
    "build": {
      "executor": "@nx/esbuild:esbuild",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/client/shared/design-system/data",
        "main": "libs/client/shared/design-system/data/src/index.ts",
        "tsConfig": "libs/client/shared/design-system/data/tsconfig.lib.json",
        "assets": ["libs/client/shared/design-system/data/*.md"],
        "format": ["cjs"],
        "generatePackageJson": true
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/client/shared/design-system/data/jest.config.ts"
      }
    }
  }
}
