{
  "name": "client-kyai-mystery-data",
  "$schema": "../../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/client/kyai/mystery/data/src",
  "projectType": "library",
  "tags": ["scope:client", "type:data"],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/client/kyai/mystery/data",
        "main": "libs/client/kyai/mystery/data/src/index.ts",
        "tsConfig": "libs/client/kyai/mystery/data/tsconfig.lib.json",
        "assets": ["libs/client/kyai/mystery/data/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": ["{projectRoot}/**/*.ts", "{projectRoot}/package.json"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/client/kyai/mystery/data/jest.config.ts"
      }
    }
  }
}
